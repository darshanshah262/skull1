#include<stdio.h>
int get_input()
{
    int a,b;
    printf("Enter first number: ");
    scanf("%d",&a);
    printf ("Enter second number: ");
    scanf ("%d",&b);
    return a;
    return b;
}

int add(int a,int b)
{
    int c;
    c=a+b;
    return c;
}

void output(int a, int b, int c)
{
    printf("%d + %d = %d",a,b,c);
}

int main(void)
{
    int a,b,c;
    get_input();
    add(a,b);
    output(a,b,c);
}
